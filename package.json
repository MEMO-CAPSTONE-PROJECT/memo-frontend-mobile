{
  "name": "memo",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "sh ./scripts/postinstall.sh",
    "dev": "expo start --dev-client --clear",
    "prod": "expo start --no-dev --minify",
    "start": "expo start",
    "prebuild": "npx expo prebuild",
    "prebuild:react-native": "npx expo prebuild --skip-dependency-update react-native,react",
    "android": "expo run:android",
    "ios": "expo run:ios --device",
    "ios:prod": "expo run:ios --device --configuration Release",
    "android:prod": "expo run:android --device --variant Release",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build:ios": "cd ios && xcodebuild -workspace ./Memo.xcworkspace -scheme Memo -configuration Release -sdk iphonesimulator CODE_SIGN_IDENTITY=\"\" CODE_SIGNING_REQUIRED=NO CODE_SIGNING_ALLOWED=NO",
    "build:apk": "cd android && ./gradlew assembleRelease",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@baronha/react-native-multiple-image-picker": "^2.2.0",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-navigation/native": "^6.0.2",
    "@tamagui/config": "^1.117.1",
    "@tamagui/core": "^1.117.1",
    "@tamagui/portal": "^1.117.1",
    "@tanstack/react-query": "^5.61.5",
    "axios": "^1.7.8",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "expo": "51.0.39",
    "expo-camera": "~15.0.16",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.27",
    "expo-font": "~12.0.9",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.24",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.28",
    "expo-web-browser": "~13.0.3",
    "jwt-decode": "^4.0.0",
    "nativewind": "^4.0.1",
    "phosphor-react-native": "^2.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "~0.75.2",
    "react-native-calendars": "^1.1309.1",
    "react-native-gesture-handler": "^2.23.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-nitro-modules": "^0.22.1",
    "react-native-permissions": "^5.2.5",
    "react-native-pie-chart": "^3.0.2",
    "react-native-qrcode-svg": "^6.3.12",
    "react-native-reanimated": "^3.16.7",
    "react-native-reanimated-skeleton": "^1.5.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "^4.6.0",
    "react-native-svg": "15.2.0",
    "react-native-toast-message": "^2.2.1",
    "react-native-web": "~0.19.10",
    "tailwindcss": "^3.4.13",
    "tamagui": "^1.117.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
